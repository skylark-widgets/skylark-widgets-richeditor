{
  "name": "skylark-widgets-wordpad",
  "description": "The skylark richeditor widget",
  "version": "0.9.0",
  "keywords": [
    "css",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://github.com/skylark-widgets/skylark-widgets-wordpad/",
  "author": "Hudaokeji, Inc.",
  "license": "MIT",
  "scripts": {
    "build": "slib build ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skylark-widgets/skylark-widgets-wordpad.git"
  },
  "bugs": {
    "url": "https://github.com/skylark-widgets/skylark-widgets-wordpad/issues"
  },
  "dependencies": {
    "skylark-codemirror": "git+https://github.com/skylark-integration/skylark-codemirror.git#master",
    "skylark-domx-contents": "git+https://github.com/skylark-domx/skylark-domx-contents.git#master",
    "skylark-domx-files": "git+https://github.com/skylark-domx/skylark-domx-files.git#master",
    "skylark-domx-panels": "git+https://github.com/skylark-domx/skylark-domx-panels.git#master",
    "skylark-domx-plugins-panels": "git+https://github.com/skylark-domx/skylark-domx-panels.git#master",
    "skylark-domx-tables": "git+https://github.com/skylark-domx/skylark-domx-tables.git#master",
    "skylark-io-diskfs": "git+https://github.com/skylark-io/skylark-io-diskfs.git#master",
    "skylark-langx": "git+https://github.com/skylark-langx/skylark-langx.git#master",
    "skylark-texts-html": "git+https://github.com/skylark-texts/skylark-texts-html.git#master",
    "skylark-widgets-base": "git+https://github.com/skylark-widgets/skylark-widgets-base.git#master",
    "skylark-widgets-toolbars": "git+https://github.com/skylark-widgets/skylark-widgets-toolbars.git#master"
  },
  "skylarkjs": {
    "directories": {
      "dependencies": "./node_modules"
    },
    "secondaries": {},
    "build": {
      "prepare": {
        "texttojs": [
          "html",
          "json",
          "css"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      },
      "bundles": {
        "all": true
      }
    }
  },
  "engines": {
    "node": ">=0.10.1"
  }
}
